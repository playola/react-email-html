{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/prefer-default-export": "off",
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": [
          ".storybook/**",
          "docs/**/*.(stories|story).(js|mdx)",
          "src/**/*.(stories|story).(js|mdx)"
        ]
      }
    ]
  }
}
